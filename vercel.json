{
    "framework": "vite",
    "functions": {
        "api.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "rewrites": [
        {
            "source": "/dhexstream/api.php",
            "destination": "/api.php"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}